<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="users.css">
    <title>Document</title>
</head>
<body>
<div>

{{#each filter}}
    <div class="users">
        <spam>{{this.id}}</spam>
    <spam>{{this.firstName}}</spam>
    <spam>{{this.lastName}}</spam>
    <spam>{{this.email}}</spam>
    <spam>{{this.password}}</spam>
    <spam>{{this.age}}</spam>
    <spam>{{this.city}}</spam>

        <form action="/delete/{{this.id}} "method="post" >
            <input type="submit" value="delete" >
        </form>
    </div>

</div>
{{/each}}
</body>
</html>